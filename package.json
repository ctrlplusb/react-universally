  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
